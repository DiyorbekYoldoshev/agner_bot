ormats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="personal_results" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="29"/><column index="2" value="52"/><column index="3" value="52"/><column index="4" value="70"/><column index="5" value="72"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">-- ========================
-- 1. SHAXSIY FOYDALANUVCHI UCHUN
-- ========================
CREATE TABLE personal_users (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    full_name TEXT NOT NULL,
    username TEXT UNIQUE,
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP
);

CREATE TABLE personal_plans (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    user_id INTEGER,
    plan_type TEXT CHECK(plan_type IN ('kunlik','haftalik','oylik')),
    title TEXT NOT NULL,
    description TEXT,
    status TEXT DEFAULT 'kutilmoqda' CHECK(status IN ('kutilmoqda','bajarildi','bekor qilindi')),
    start_date DATE,
    end_date DATE,
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    FOREIGN KEY (user_id) REFERENCES personal_users(id)
);

CREATE TABLE personal_results (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    user_id INTEGER,
    plan_id INTEGER,
    result_text TEXT,
    result_date DATE DEFAULT CURRENT_DATE,
    FOREIGN KEY (user_id) REFERENCES personal_users(id),
    FOREIGN KEY (plan_id) REFERENCES personal_plans(id)
);

-- ========================
-- 2. BIZNES FOYDALANUVCHILARI UCHUN
-- ========================
CREATE TABLE business_users (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    full_name TEXT NOT NULL,
    username TEXT UNIQUE,
    role TEXT CHECK(role IN ('admin','hisobchi','hodim')),
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP
);

CREATE TABLE business_tasks (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    assigned_to INTEGER,
    assigned_by INTEGER,
    task_type TEXT CHECK(task_type IN ('kunlik','haftalik','oylik')),
    title TEXT NOT NULL,
    description TEXT,
    status TEXT DEFAULT 'kutilmoqda' CHECK(status IN ('kutilmoqda','bajarildi','bekor qilindi')),
    start_date DATE,
    end_date DATE,
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    FOREIGN KEY (assigned_to) REFERENCES business_users(id),
    FOREIGN KEY (assigned_by) REFERENCES business_users(id)
);

CREATE TABLE business_reports (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    user_id INTEGER,
    task_id INTEGER,
    work_hours REAL DEFAULT 0,
    result_text TEXT,
    result_date DATE DEFAULT CURRENT_DATE,
    FOREIGN KEY (user_id) REFERENCES business_users(id),
    FOREIGN KEY (task_id) REFERENCES business_tasks(id)
);
</sql><current_tab id="0"/></tab_sql></sqlb_project>
